{
    // Plugin identifiers are globally unique and only contain A-Za-z0-9_-
    "id": "elasticsearch-index-template",

    // It is highly recommended to use Semantic Versioning
    "version": "0.0.1",

    // Meta data for display purposes:
    "meta": {
        // label: name of the plugin as displayed, should be short
        "label": "Elasticsearch index template",

        // description: longer string to help end users understand what this plugin does
        "description": "Plugin customizing Elasticsearch mappping for a DSS dataset to avoid mapping conflict when an index template is configured in the target Elasticsearch cluster.",

        "author": "admin",

        // icon: must be one of the FontAwesome 3.2.1 icons, complete list here at https://fontawesome.com/v3.2.1/icons/
        "icon": "icon-puzzle-piece",

        // List of tags for filtering the list of plugins
        "tags": ["Elasticsearch"], //for example: ["Machine Learning", "NLP"]

        // URL where the user can learn more about the plugin
        "url": "",

        "licenseInfo": "Apache Software License"
    },
    
    "params": [
        {
            "name": "columns",
            "label": "Columns names",
            "type": "STRING",
            "mandatory": false,
            "description": "Optional. Comma-separated list of column names defined in  elasticsearch index template conflicting with DSS column mapping."
        },
        {
          "name": "es-connections",
          "label": "Elasticsearch connections",
          "type": "CONNECTIONS",
          "description": "Optional. Elasticsearch connections containing an Elasticsearch index template."
        }
    ]
}
